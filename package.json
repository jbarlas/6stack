{
  "name": "6stack",
  "version": "1.0.0",
  "description": "Overwatch scouting and scrim finder built using MERN stack",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client", 
    "dev": "concurrently \"npm run server\" \"npm run client \"",  
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jbarlas/6stack.git"
  },
  "author": "Justin Barlas",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jbarlas/6stack/issues"
  },
  "homepage": "https://github.com/jbarlas/6stack#readme",
  "dependencies": {
    "body-parser": "^1.19.1",
    "concurrently": "^7.0.0",
    "express": "^4.17.2",
    "mongoose": "^6.2.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.15"
  }
}
